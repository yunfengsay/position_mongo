position.group {
    gzip
    proxy / http://www.google.com

    log /var/log/caddy/position.group.log
    header /api {
         Access-Control-Allow-Origin  *
         Access-Control-Allow-Methods "GET, POST, OPTIONS"
         -Server
    }
}

api.position.group {
    proxy / 127.0.0.1:8002 {
            transparent
    }
}
